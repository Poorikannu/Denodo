CREATE OR REPLACE WRAPPER DF ned
    DATASOURCENAME=ned
    OUTPUTSCHEMA (
        "<!DOCTYPE html>" = '<!DOCTYPE html>' (OPT)
    );

CREATE OR REPLACE TABLE ned I18N za (
        "<!DOCTYPE html>":text
    )
    CACHE OFF
    TIMETOLIVEINCACHE DEFAULT
    ADD SEARCHMETHOD ned(
        I18N za
        CONSTRAINTS (
             ADD "<!DOCTYPE html>" (any) OPT ANY
        )
        OUTPUTLIST ("<!DOCTYPE html>"
        )
        WRAPPER (df ned)
    );

