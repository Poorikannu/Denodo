CREATE OR REPLACE WRAPPER DF dhan
    DATASOURCENAME=dhan
    OUTPUTSCHEMA (
        "Column0" = 'Column0' (OPT)
    );

CREATE OR REPLACE TABLE dhan I18N au_cst (
        "Column0":text
    )
    CACHE OFF
    TIMETOLIVEINCACHE DEFAULT
    ADD SEARCHMETHOD dhan(
        I18N au_cst
        CONSTRAINTS (
             ADD "Column0" (any) OPT ANY
        )
        OUTPUTLIST ("Column0"
        )
        WRAPPER (df dhan)
    );

