# REQUIRES-PROPERTIES-FILE - # Do not remove this comment!
CREATE OR REPLACE WRAPPER JDBC "CURRENT_TIMESTAMP_DEMOS"
    DATASOURCENAME=oracle19c_copy
    SCHEMANAME='${databases.test_vcs_db.views.jdbc.current_timestamp_demos.SCHEMANAME}' ESCAPE
    RELATIONNAME='CURRENT_TIMESTAMP_DEMOS' 
    OUTPUTSCHEMA (
        "ID" = 'ID' :'java.lang.Long' (OPT) (sourcetypedecimals='0', sourcetyperadix='10', sourcetypesize='10', sourcetypeid='2', sourcetypename='NUMBER')  SORTABLE,
        "MSG" = 'MSG' :'java.lang.String' (OPT) (sourcetyperadix='10', sourcetypesize='255', sourcetypeid='12', sourcetypename='VARCHAR2')  SORTABLE,
        "CREATED_AT" = 'CREATED_AT' :'java.time.LocalDateTime' (OPT) (sourcetypedecimals='3', sourcetyperadix='10', sourcetypesize='11', sourcetypeid='93', sourcetypename='TIMESTAMP(3)')  SORTABLE
    );

CREATE OR REPLACE TABLE "CURRENT_TIMESTAMP_DEMOS" I18N jp (
        "ID":long (sourcetypeid = '-5', sourcetyperadix = '10', sourcetypedecimals = '0', sourcetypesize = '10'),
        "MSG":text (sourcetypeid = '12', sourcetyperadix = '10', sourcetypesize = '255'),
        "CREATED_AT":timestamp (sourcetypeid = '93', sourcetyperadix = '10', sourcetypedecimals = '3', sourcetypesize = '11')
    )
    CACHE OFF
    TIMETOLIVEINCACHE DEFAULT
    ADD SEARCHMETHOD "CURRENT_TIMESTAMP_DEMOS"(
        I18N jp
        CONSTRAINTS (
             ADD "ID" (any) OPT ANY
             ADD "MSG" (any) OPT ANY
             ADD "CREATED_AT" (any) OPT ANY
        )
        OUTPUTLIST ("CREATED_AT", "ID", "MSG"
        )
        WRAPPER (jdbc "CURRENT_TIMESTAMP_DEMOS")
    );

